      "\t\"blocks\": [",
      "\t\t{",
      "\t\t\t\"type\": \"@theme\"",
      "\t\t},",
      "\t\t{",
      "\t\t\t\"type\": \"@app\"",
      "\t\t}",
      "\t],",
      "\t\"presets\": [",
