    "prefix": ["ifb", "ibtn", "ifbutton"],
    "body": [
      "{% if ${1:section.settings.link} != blank and ${2:section.settings.cta} != blank %}",
      "\t<a href='{{ ${1:section.settings.link} }}' class='${TM_FILENAME_BASE}__button td-button$3'>{{ ${2:section.settings.cta} }}</a>",
      "{% endif %}"
    ]
  },
  "Attributes attribute": {
    "prefix": ["attrs", "attributes"],
    "body": [
      " style=\"{% render 'td-style-attributes', settings: section.settings %}\""
    ]
  },
  "Duplicate comment": {
    "prefix": ["dupe"],
    "body": [
      "{% comment %} Copy of ${TM_FILENAME/(^td-)?(.*?)/$2/} {% endcomment %}"
    ]
  },
  "Duplicate text": {
    "prefix": ["dupetext", "dtext"],
    "body": [
      "{% comment %} Copy of ${TM_FILENAME/(^td-)?(.*?)/$2/} {% endcomment %}"
    ]
  },
  "Themecheck disable": {
    "prefix": ["disable", "themecheckdisable"],
    "body": ["{% # theme-check-disable %}"]
  },
  "Theme check enable": {
    "prefix": ["enable", "themecheckenable"],
    "body": ["{% # theme-check-enable %}"]
  },
  "Metaobject loop": {
    "prefix": ["mloop", "metaobjectloop"],
    "body": [
      "{% for object in shop.metaobjects.${1:object_type}.values %}",
      "\t$0",
      "{% endfor %}"
    ]
  },
  "Richtext metaoject loop": {
    "prefix": ["rml", "mlr", "rmetaobjectloop"],
    "body": [
      "{% for object in shop.metaobjects.${1:object_type}.values %}",
      "\t{{ object.${2:text} | metafield_tag }}$0",
      "{% endfor %}"
    ]
  },
  "Link Option": {
    "prefix": ["lopt", "link option"],
    "body": [
      "{% if ${1:section.settings.url} != blank %}",
      "\t<a href=\"{{ ${1:section.settings.url} }}\" class=\"${TM_FILENAME_BASE}__${2:container} ${TM_FILENAME_BASE}__${2:container}--link\">",
      "{% else %}",
      "\t<div class=\"${TM_FILENAME_BASE}__${2:container}\">",
      "{% endif %}",
      "\t$0",
      "{% if ${1:section.settings.url} != blank %}",
      "\t</a>",
      "{% else %}",
      "\t</div>",
      "{% endif %}"
    ]
  },
  "If Main Title": {
    "prefix": ["mt", "ifmt", "imt", "ifmain"],
    "body": [
      "{% if ${1:section.settings.title} != blank %}",
      "\t{% if section.index == 1 %}<h1{% else %}<h2{% endif %} class='${TM_FILENAME_BASE}__title td-title$2'>{{ ${1:section.settings.title} }}{% if section.index == 1 %}</h1{% else %}</h2{% endif %}>",
      "{% endif %}"
    ]
  },
  "Button Schema": {
    "prefix": ["_button"],
    "body": [
      "{",
      "\t\"type\": \"header\",",
      "\t\"content\": \"Button\"",
      "},",
      "{",
      "\t\"type\": \"url\",",
      "\t\"id\": \"link\",",
      "\t\"label\": \"Button link\"",
      "},",
      "{",
      "\t\"type\": \"text\",",
      "\t\"id\": \"cta\",",
      "\t\"label\": \"Button CTA\"",
      "},"
    ]
  },
  "Collection List Schema": {
    "prefix": ["_cl", "_clist"],
    "body": [
      "{",
      "\t\"type\": \"collection_list\",",
      "\t\"id\": \"collection_list\",",
      "\t\"label\": \"Collections\"",
      "}"
    ]
  }
}
