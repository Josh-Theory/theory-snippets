  "Swiper Block Content": {
    "prefix": ["cbsp", "contentblockswiper", "cfbsp"],
    "body": [
      "{% # theme-check-disable %}",
      "{% comment %} Disable 'content_for block capture' check {% endcomment %}",
      "{% liquid",
      "\tif section.blocks.size > 0",
      "\t\tcapture slide_contents",
      "\t\t\tcontent_for 'blocks'",
      "\t\tendcapture",
      "\t\tcontent_for 'block', type: '_td-slider', id: 'static-slider', children: slide_contents, classes: '${TM_FILENAME_BASE}__slider'",
