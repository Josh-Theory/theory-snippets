      "\t\t\t\"max\": 100,",
      "\t\t\t\"step\": 1,",
      "\t\t\t\"unit\": \"px\",",
      "\t\t\t\"default\": 0,",
      "\t\t\t\"visible_if\": \"{{ section.settings.custom_padding_mobile }}\"",
      "\t\t}",
