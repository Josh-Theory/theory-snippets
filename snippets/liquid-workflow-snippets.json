  "Legacy Swiper Boilerplate": {
    "prefix": ["lswp", "lswiper"],
    "body": [
      "{%- capture sliderOptions -%}",
      "\t{",
      "\t\t\"centeredSlides\": true,",
      "\t\t\"slidesPerView\": 1,",
      "\t\t\"spaceBetween\": 10,",
      "\t\t\"loop\": {{ section.settings.loop }},",
      "\t\t\"breakpoints\": {",
      "\t\t\t\"450\": {",
      "\t\t\t\t\"slidesPerView\": \"auto\"",
      "\t\t\t}",
      "\t\t}",
      "\t}",
      "{%- endcapture -%}",
      "",
      "{% if ${1:section.blocks.size > 0} %}",
      "\t<swiper-slider data-swiper-options='{{sliderOptions}}'>",
      "\t\t<div class=\"swiper ${TM_FILENAME_BASE}__swiper\" data-swiper>",
      "\t\t\t<div class=\"swiper-wrapper ${TM_FILENAME_BASE}__swiper-wrapper\">",
      "\t\t\t\t{% for ${2:block} in ${3:section.blocks} %}",
      "\t\t\t\t\t<div class=\"swiper-slide ${TM_FILENAME_BASE}__slide\" data-swiper-slide>",
      "\t\t\t\t\t\t$0",
      "\t\t\t\t\t</div>",
      "\t\t\t\t{% endfor %}",
      "\t\t\t</div>",
      "\t\t</div>",
      "\t</swiper-slider>",
      "{% endif %}"
    ]
  },
